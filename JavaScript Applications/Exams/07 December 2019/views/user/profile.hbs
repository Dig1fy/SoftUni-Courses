{{> header}}
 
 <div class="profile col-md-6 text-center col-lg">
    <img class="profile-img" src="./images/user.png" />
    <div class="profile-info">
      <p>Username: <small>{{username}}</small></p>
      <p class="infoType">Wished {{treksNumber}} treks =)</p>

      {{#each treks}}
      <p>{{this}}</p>
      {{else}}
      <p>No treks</p>
      {{/each}}
    </div>
  </div>

 {{> footer}}